{
    "env": {
        "es2021": true,
        "node": true,
        "vitest-globals/env": true
    },
    "extends": ["airbnb-base", "plugin:vitest-globals/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["node_modules", "submissions"],
    "rules": {
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "max-len": "off"
    }
}
